                  <DropShadow color="#0000004d" height="0.0" offsetY="5.0" radius="0.0" width="0.0" />
               </effect>
            </Button>
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onLevelEditor" stylesheets="@../style.css" text="Level Editor" />
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onExit" stylesheets="@../style.css" text="Exit" />
